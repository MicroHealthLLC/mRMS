      <a class="nav-link my_reports " id="pills-<%= channelEnum.id %>-tab" data-toggle="tab" href="#datasets<%= channelEnum.id %>" role="tab" aria-controls="pills-<%= channelEnum.id %>" aria-selected="true" draggable="false" ondrop="drop_report(event,<%= channelEnum.id %>)" ondragover="allowDrop_report(event)"> <%= channelEnum.name %> </a>
