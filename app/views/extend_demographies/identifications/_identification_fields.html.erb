<div class="row">
  <fieldset>

    <div class="col-md-12">
      <div class="  form-group col-md-12">
        <%= f.label :identification_type_id, class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.select :identification_type_id, options_for_select(IdentificationType.active.pluck(:name, :id), selected: f.object.identification_type.try(:id) ), {} , {class: 'form-control'}%>
        </div>
      </div>

      <div class="form-group col-md-12">
        <%= f.label :identification_number, class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :identification_number,  class: 'form-control' %>
        </div>
      </div>

      <div class="  form-group col-md-12">
        <%= f.label :status, class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.select :status, [['Active', true], ['Inactive', false]] ,{},  {class: 'form-control'} %>
        </div>
      </div>

      
      <div class="  form-group col-md-12">
        <%= f.label :date_expired , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :date_expired ,  class: 'form-control date_picker' %>
        </div>
      </div>

      <div class="  form-group col-md-12">
        <%= f.label :issued_by_type , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.select :issued_by_type_id , options_for_select(IssuedByType.active.pluck(:name, :id), selected: f.object.issued_by_type.try(:id)) ,{},  {class: 'form-control'} %>
        </div>
      </div>
      <div class="  form-group col-md-12">
        <%= f.label :date_issued , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_field :date_issued ,  class: 'form-control date_picker' %>
        </div>
      </div>

      <div class="  form-group col-md-12">
        <%= f.label :note , class: 'col-md-2 control-label' %>
        <div class="col-md-8">
          <%= f.text_area :note ,  class: 'form-control' %>
        </div>
      </div>
    </div>
  </fieldset>
</div>
