<div class="row">
  <div class="col-md-12 ">
    <div class="pull-right">
      <% if @channel.is_personal? and @channel.is_creator?  %>
          <span>
            <%= show_button(share_report_channel_report_path(@channel, @report), ' Share report', 'plus', 'primary') %>
          </span>
      <% end %>

      <%  if @report.document_url %>
          <%= show_button('#', ' New Report', 'arrow-down', 'info', {toggle:"modal", target:"#mySaveModal" }) %>
          <%= show_button('#', ' Refresh Report', 'arrow-up', 'info', {toggle:"modal", target:"#myChangeModal" }) %>
      <% end %>

      <% if @channel.is_creator? or @channel.my_permission.can_add_report? %>
              <span>
                <%= show_button('#', ' Upload Data', 'upload', 'info', {toggle:"modal", target:"#myModal" }) %>
              </span>
      <% end %>


      <% if @channel.is_creator? or (@channel.my_permission.can_add_report? ) %>
          <span>
            <%= edit_button edit_channel_report_path(@channel, @report) %>
          </span>
      <% end %>

      <% if  @channel.is_creator? or (@channel.my_permission.can_delete_report? or @report.can?(:manage_roles) ) %>
          <span>
            <%= delete_button channel_report_path(@channel, @report) %>
          </span>
      <% end %>
      <%= cancel_button channel_path(@channel) %>
      <% if params[:query_id] %>
              <span>
                <%= show_button(channel_report_path(@channel, @report), ' Clear', 'times', 'default') %>
              </span>
      <% end %>


      <%= render partial: 'uploader/report_upload' %>
    </div>
  </div>
</div>


